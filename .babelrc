{
  "presets": [
      ["@babel/preset-env", {
          "loose": true,
          "modules": false,
          "targets": "> 0.25%, not dead"
      }],
      "@babel/preset-react",
      "@babel/preset-typescript"
  ],
  "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
  ],
  "ignore": [
    "node_modules"
  ]
}
